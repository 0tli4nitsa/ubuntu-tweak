[Desktop Entry]
Name=Ubuntu Tweak
_Comment=Tweak ubuntu to what you like
Exec=ubuntu-tweak
Icon=ubuntu-tweak
Terminal=false
Type=Application
Categories=GNOME;GTK;System;
StartupNotify=true
X-Ayatana-Desktop-Shortcuts=Compiz;Icon;DesktopRecovery;FileType;Shortcuts;Nautilus
X-Ayatana-Appmenu-Show-Stubs=False

[Compiz Shortcut Group]
_Name=Compiz Settings
Exec=ubuntu-tweak -m Compiz
TargetEnvironment=Unity

[Icon Shortcut Group]
_Name=Desktop Icon Settings
Exec=ubuntu-tweak -m Icon
TargetEnvironment=Unity

[DesktopRecovery Shortcut Group]
_Name=Desktop Recovery
Exec=ubuntu-tweak -m DesktopRecovery
TargetEnvironment=Unity

[FileType Shortcut Group]
_Name=File Type Manager
Exec=ubuntu-tweak -m FileType
TargetEnvironment=Unity

[Shortcuts Shortcut Group]
_Name=Shortcut Commands
Exec=ubuntu-tweak -m Shortcuts
TargetEnvironment=Unity

[Nautilus Shortcut Group]
_Name=Nautilus Settings
Exec=ubuntu-tweak -m Nautilus
TargetEnvironment=Unity
