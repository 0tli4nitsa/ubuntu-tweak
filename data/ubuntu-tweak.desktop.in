[Desktop Entry]
Name=Ubuntu Tweak
_Comment=Tweak ubuntu to what you like
Exec=ubuntu-tweak
Icon=ubuntu-tweak
Terminal=false
Type=Application
Categories=GNOME;GTK;System;
StartupNotify=true
X-Ayatana-Desktop-Shortcuts=Overview;Tweaks;Admins;Janitor
X-Ayatana-Appmenu-Show-Stubs=False

[Overview Shortcut Group]
_Name=Overview
Exec=ubuntu-tweak
TargetEnvironment=Unity

[Tweaks Shortcut Group]
_Name=Tweaks
Exec=ubuntu-tweak -f tweaks
TargetEnvironment=Unity

[Admins Shortcut Group]
_Name=Admins
Exec=ubuntu-tweak -f admins
TargetEnvironment=Unity

[Janitor Shortcut Group]
_Name=Janitor
Exec=ubuntu-tweak -f janitor
TargetEnvironment=Unity
